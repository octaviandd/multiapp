{
  "name": "multiapp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon -q index.ts",
    "clean": "rm -rf dist",
    "build:prod": "npm run clean && npm run build && npm run copy-prisma",
    "copy-prisma": "mkdir -p dist/src/prisma/migrations && cp src/prisma/schema.prisma dist/src/prisma/schema.prisma && cp -r src/prisma/migrations/* dist/src/prisma/migrations/ && npx prisma generate"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.670.0",
    "@prisma/client": "^5.12.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-openid-connect": "^2.17.1",
    "express-session": "^1.18.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.4",
    "passport": "^0.7.0",
    "passport-oauth2": "^1.8.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.7",
    "@types/passport": "^1.0.16",
    "@types/passport-oauth2": "^1.4.15",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "prisma": "^5.12.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
